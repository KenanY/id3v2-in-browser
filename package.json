{
  "name": "id3v2-in-browser",
  "version": "1.0.0",
  "repository": "KenanY/id3v2-in-browser",
  "license": "MIT",
  "author": "Kenan Yildirim <kenan@kenany.me> (http://kenany.me/)",
  "private": true,
  "scripts": {
    "build": "browserify client -p bundle-collapser/plugin > static/bundle.js",
    "minify": "npm run build && uglifyjs -c warnings=false -m < static/bundle.js > static/bundle.min.js",
    "size": "npm run minify && gzip < static/bundle.min.js | wc -c | pretty-bytes",
    "start": "node server.js",
    "watch": "watchify client -o static/bundle.js -dv & DEBUG=* nodemon server.js"
  },
  "dependencies": {
    "bl": "^0.9.3",
    "drag-drop": "^2.0.0",
    "id3v2-parser": "^0.3.0",
    "non-ascii": "^1.0.0"
  },
  "devDependencies": {
    "browserify": "^7.0.0",
    "bundle-collapser": "^1.1.0",
    "node-static": "^0.7.6",
    "nodemon": "^1.2.1",
    "pretty-bytes": "^1.0.1",
    "uglifyjs": "^2.3.6",
    "watchify": "^2.1.1"
  }
}